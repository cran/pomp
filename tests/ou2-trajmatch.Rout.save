+                                 start=guess.p,
+                                 est=c('alpha.1','alpha.4','x1.0','x2.0','tau'),
+                                 maxit=2000,
+                                 reltol=1e-8
+                                 )
+               c(conv=res$convergence,loglik=res$value,res$params)
+             }
+             )
